<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings version="3.0"
               xmlns:jaxb="https://jakarta.ee/xml/ns/jaxb"
               xmlns:xs="http://www.w3.org/2001/XMLSchema"
               xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
               xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
               xmlns:simplify="http://jaxb2-commons.dev.java.net/basic/simplify"
               jaxb:extensionBindingPrefixes="xjc annox">

    <jaxws:bindings xmlns:jaxws="https://jakarta.ee/xml/ns/jaxws">
        <jaxws:enableWrapperStyle>true</jaxws:enableWrapperStyle>
        <jaxws:enableAsyncMapping>false</jaxws:enableAsyncMapping>
    </jaxws:bindings>

    <jaxb:bindings schemaLocation="OPI_GIORNALE_DI_CASSA_V_1_8.xsd" node="/xs:schema">
      <jaxb:globalBindings>
        <xjc:simple />
      </jaxb:globalBindings>
      <jaxb:schemaBindings>
        <jaxb:package name="it.gov.pagopa.payhub.activities.xsd.treasury.opi18" />
      </jaxb:schemaBindings>
      <jaxb:bindings node="xs:element[@name='flusso_giornale_di_cassa']/xs:complexType" >
          <simplify:property name="signaturesAndInformazioniContoEvidenzasAndDataRiferimentoGdCs">
            <simplify:as-element-property/>
          </simplify:property>
      </jaxb:bindings>
    </jaxb:bindings>

</jaxb:bindings>